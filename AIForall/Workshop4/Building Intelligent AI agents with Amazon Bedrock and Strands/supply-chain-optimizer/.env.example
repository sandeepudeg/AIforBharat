# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_id
AWS_SECRET_ACCESS_KEY=your_secret_access_key

# RDS Configuration
RDS_HOST=your-rds-endpoint.rds.amazonaws.com
RDS_PORT=5432
RDS_DATABASE=supply_chain_db
RDS_USERNAME=postgres
RDS_PASSWORD=your_password

# DynamoDB Configuration
DYNAMODB_REGION=us-east-1
DYNAMODB_ENDPOINT=http://localhost:8000

# S3 Configuration
S3_BUCKET_NAME=supply-chain-optimizer-reports
S3_REGION=us-east-1

# SNS Configuration
SNS_TOPIC_ARN_ALERTS=arn:aws:sns:us-east-1:123456789012:supply-chain-alerts
SNS_TOPIC_ARN_NOTIFICATIONS=arn:aws:sns:us-east-1:123456789012:supply-chain-notifications

# Lambda Configuration
LAMBDA_ROLE_ARN=arn:aws:iam::123456789012:role/supply-chain-lambda-role

# EventBridge Configuration
EVENTBRIDGE_RULE_NAME=supply-chain-events

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json

# Strands Agents Configuration
BEDROCK_MODEL_ID=anthropic.claude-3-sonnet-20240229-v1:0
BEDROCK_REGION=us-east-1

# Application Configuration
NODE_ENV=development
